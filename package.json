{
  "name": "weatherapp",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "client-install": "npm install --prefix client",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigchsta62/notFakeNewsAndWeather.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bigchsta62/notFakeNewsAndWeather/issues"
  },
  "homepage": "https://github.com/bigchsta62/notFakeNewsAndWeather#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "env": "0.0.2",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "moment": "^2.29.1",
    "nodemon": "^2.0.7",
    "react-geolocated": "^3.1.0",
    "react-moment": "^1.1.1"
  }
}
